    "preview": "vite preview",
    "sort-json": "npx sort-package-json \"*.json\" \"*(?!node_modules)/*.json\"",
    "stylelint": "npx stylelint --cache --cache-location \"node_modules\\.cache\\.stylelintcache\" --max-warnings 0 {src,mock}/**/*.{scss,sass,less,css,html,vue,xml,svelte,php,twig,jade,json,md,yml,yaml,ini,toml,hbs,ejs,pug,jinja,j2,j3} --fix",
